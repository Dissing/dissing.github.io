<?xml version="1.0" ?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>Lasse Dissing</title>
    <link>http://lassedissing.com</link>
    <description></description>
    <language>en-UK</language>
    {% for post in site.posts %}
    <item>
      <title>{{ post.title }}</title>
      <link>http://lassedissing.com{{ post.url }}</link>
      <guid>http://lassedissing.com{{ post.url }}</guid>
      <description><![CDATA[{{ post.excerpt }}]]></description>
      <pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
    </item>
    {% endfor %}
    <atom:link href="http://lassedissing.com/rss.xml" rel="self" type="application/rss+xml"/>
  </channel>
</rss>
